// Print the fibonacci sequence up to a given number
fun main() {
    val n = 20 // replace with your desired limit
    var t1 = 0
    var t2 = 1
    print("Fibonacci Series up to $n: ")

    while (t1 <= n) {
        print("$t1, ")

        val sum = t1 + t2
        t1 = t2
        t2 = sum
    }
}
